<div class="jumbotron">
  <%= form_with model: @todo, local: true do |f| %>
    <%= f.label :description, "description:" %>
    <%= f.text_field :description, required: true %>
    <%= f.label :completed, "Finalizado:" %>
    <%= f.check_box :completed %>
    <%= f.submit "Agregar" %>
  <% end %>
</div>
